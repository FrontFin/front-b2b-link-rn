{
  "name": "@front-finance/frontfinance-rn-sdk",
  "version": "1.1.0",
  "description": "Front Finance React Native SDK.",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && yarn build:esm && yarn build:cjs && yarn copy",
    "build:esm": "tsc --outDir dist",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "typecheck": "tsc --noEmit",
    "copy": "node ./tools/copy",
    "publish:npm": "yarn build && cd dist && yarn publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrontFin/front-b2b-link-rn.git"
  },
  "keywords": [
    "front-finance"
  ],
  "homepage": "https://github.com/FrontFin/front-b2b-link-rn#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "react": "^18.2.0",
    "react-native": "^0.72.3"
  },
  "dependencies": {
    "react-native-webview": "11.26.0"
  }
}
