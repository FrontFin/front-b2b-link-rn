{
  "version": "1.2.1",
  "name": "@front-finance/frontfinance-rn-sdk",
  "description": "Front Finance React Native SDK.",
  "private": false,
  "license": "MIT",
  "author": "Front Finance, Inc",
  "repository": "https://github.com/FrontFin/front-b2b-link-rn.git",
  "homepage": "https://github.com/FrontFin/front-b2b-link-rn",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/frontfinance-rn-sdk.esm.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && node scripts/build.js",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build && yarn lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "publish:npm": "yarn build && cd dist && yarn publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "front-finance"
  ],
  "size-limit": [
    {
      "path": "dist/frontfinance-rn-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "react-native-webview": "^11.26.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-native": ">=0.60"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/react": "^18.2.20",
    "@types/react-native": "^0.72.2",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-native": "^0.72.3",
    "size-limit": "^8.2.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  }
}
