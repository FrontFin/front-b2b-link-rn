{
  "version": "1.2.1",
  "name": "@front-finance/frontfinance-rn-sdk",
  "description": "Front Finance React Native SDK.",
  "private": false,
  "license": "MIT",
  "author": "Front Finance, Inc",
  "repository": "https://github.com/FrontFin/front-b2b-link-rn.git",
  "homepage": "https://github.com/FrontFin/front-b2b-link-rn",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/frontfinance-rn-sdk.esm.js",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsc --build && node scripts/build.js",
    "check-build": "node scripts/verify-build.js",
    "clean": "tsc --build --clean",
    "lint": "eslint . --format=node_modules/eslint-formatter-pretty",
    "size": "size-limit",
    "test": "jest --passWithNoTests",
    "publish:npm": "yarn build && cd dist && yarn publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "front-finance"
  ],
  "size-limit": [
    {
      "path": "dist/frontfinance-rn-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "react-native-webview": "^11.26.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-native": ">=0.60"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@size-limit/esbuild": "^8.2.6",
    "@size-limit/esbuild-why": "^8.2.6",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@testing-library/react-native": "^12.2.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/node": "^20.5.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-test-renderer": "^18.2.0",
    "size-limit": "^8.2.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
